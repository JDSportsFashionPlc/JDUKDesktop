<script>
    var checkRefer = function() {
        if (window.opener == null) {
            var el = document.getElementById('close');
                el.innerHTML = 'Go to JDSports.co.uk';
                el.href = 'http://www.jdsports.co.uk';
                el.style.backgroundImage = 'url(images/home-icon.png)';
                el.setAttribute('onClick', '');
        }
    }
